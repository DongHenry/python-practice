    "# Function argument unpacking in Python\n",
    "\n",
    "def my_func(x, y, z):\n",
    "    print(x, y, z)\n",
    "    \n",
    "tuple_vec = (1, 2, 3)\n",
    "dict_vec = {'x': 1, 'y': 2, 'z': 3}\n",
    "\n",
    "# tuple as the argument\n",
    "my_func(*tuple_vec)\n",
    "\n",
    "# dict as the argument\n",
    "my_func(**dict_vec)"
   ]
  }
